{
  "name": "vercel-dart",
  "version": "1.0.5",
  "description": "Vercel Runtime for Dart serverless functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Frenco <hey@frenco.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frencojobs/vercel-dart"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "cpy src '!**/*.ts' dist",
    "prerelease": "yarn build && cpy ../README.md .",
    "release": "yarn publish",
    "postrelease": "rimraf README.md",
    "vercel-build": "yarn build && cd web && yarn install && yarn build"
  },
  "dependencies": {
    "execa": "^5.0.0",
    "fast-glob": "^3.2.5",
    "js-yaml": "^4.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.22",
    "@vercel/build-utils": "^2.8.0",
    "@vercel/frameworks": "^0.2.0",
    "@vercel/routing-utils": "^1.9.2",
    "cpy-cli": "^3.1.1",
    "import-sort-style-absolute": "^1.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "absolute",
      "parser": "typescript"
    }
  }
}
